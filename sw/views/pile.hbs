    async function loadBalance(){
        const currentBalance = document.querySelector('.currentBalance');
        currentBalance.addEventListener('onload', async () => {
            const account = window.ethereum.selectedAddress;
            const balance = await window.contract.methods.balanceOf(account).call();
            document.querySelector('.currentBalance').innerHTML = balance + ' SWYFI';
        });
    }

    async function loadRewards(){
        const currentRewards = document.querySelector('.currentRewards');
        currentRewards.addEventListener('onload', async () => {
            const account = window.ethereum.selectedAddress;
            const rewards = await window.contract.methods.rewards(account).call();
            document.querySelector('.currentRewards').innerHTML = rewards + ' SWYFI';
        });
    }
    async function load() {
        window.web3 = new Web3(window.ethereum);
        window.ethereum.enable();
        loadBalance();
        window.contract = await loadsmartcontract();
    }
