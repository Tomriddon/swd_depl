<script>
    const sendEthButton = document.querySelector('.pile');
    sendEthButton.addEventListener('click', async () =>{
        const amount = ((parseInt(document.querySelector('.inputfield').value) != "" )||(parseInt(document.querySelector('.inputfield').value) > 0 ))? parseInt(document.querySelector('.inputfield').value):0;
        const value = Math.floor(amount);
        const account = window.ethereum.selectedAddress;
        await window.contract.methods.stake(value).send({ from: account });
        const balance = await window.contract.methods.balanceOf(account).call();
        document.querySelector('.currentBalance').innerHTML = balance + ' SWYFI';
        const rewards = await window.contract.methods.rewards(account).call();
        document.querySelector('.currentRewards').innerHTML = rewards + ' SWYFI';
    });
</script>

